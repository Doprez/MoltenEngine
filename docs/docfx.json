{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": true,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net7.0"
      }
    }
  ],
  "build": {
    "content":
      [
        {
          "files": [
            "api/**.yml", 
            "api/**.md"
          ]
        },
        {
          "files": [
            "toc.yml", 
            "*.md"
          ]
        }
      ],
    "resource": [
        {
          "files": ["spec/images/**"]
        }
    ],
    "externalReference": [
    ],
    "globalMetadata": {
      "_appTitle": "Molten Engine",
      "_appFooter": "<span>Copyright Â© 2023 James Yarwood<br>Generated by <strong>DocFX</strong></span>",
      "_gitContribute": {
        "repo": "https://github.com/Syncaidius/MoltenEngine",
        "branch": "master",
        "apiSpecFolder": "docs-ref-overwrite",
        "_enableSearch": true
      }
    },
    "dest": "../docs/",
    "template": "molten-template",
    "sitemap": {
      "baseUrl": "https://syncaidius.github.io/MoltenEngine/",
      "priority": 0.1,
      "changefreq": "monthly"
    }
  }
}